{
  "name": "skypad-ide-backend",
  "version": "0.2.0",
  "private": true,
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "code-editor": "node src/codeEditorServer.js",
    "dev:code-editor": "nodemon src/codeEditorServer.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:code-editor\"",
    "build": "echo 'No build step required for Node.js'",
    "render-postbuild": "npm install pm2 -g",
    "render-start": "pm2 start ecosystem.config.js --no-daemon"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "google-auth-library": "^10.3.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.18.0",
    "mongoose": "^8.7.0",
    "morgan": "^1.10.0",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}